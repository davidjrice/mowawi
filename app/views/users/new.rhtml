<h2>Sign up</h2>

<p>If you have an OpenID account, <%= link_to "Login with OpenID", new_session_path %></p>

<%= error_messages_for :user %>

<% form_for :user, :url => users_path do |f| -%>

  <%= render :partial => 'form', :object => f %>
  <p><%= submit_tag 'Sign up' %></p>

<% end #form_for -%>
